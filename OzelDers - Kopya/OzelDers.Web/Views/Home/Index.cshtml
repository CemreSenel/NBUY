@using OzelDers.Web.Models.Dtos
@model List<TeacherDto>

<div class="text-center">
    <h1 class="display-6">Welcome OzelDers</h1>
    <div class="mt-4">
        <div class="row">
            @foreach (var teacherDto in Model)
            {
                <div class="col-md-4">
                    @await Html.PartialAsync("_TeacherPartial", teacherDto)
                </div>
            }
        </div>
    </div>
</div>
